{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "content_scripts": [
        {
          "js": ["popup.ts"],
          "matches": [
            "https://developer.chrome.com/docs/extensions/*",
            "https://developer.chrome.com/docs/webstore/*"
          ]
        }
      ],
    "action": {
      "default_popup": "hello.html",
      "default_icon": "hello.png"
    }
  }